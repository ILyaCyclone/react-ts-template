{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "ilyacyclone",
  "license": "ISC",
  "scripts": {
    "start": "webpack serve --hot --mode=development",
    "build": "npm run clean && webpack --mode=production",
    "build:dev": "npm run clean && webpack --mode=development",
    "watch": "webpack --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json --cache ./",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.json --cache --fix ./",
    "ts-example:start": "npx ts-node src/ts-example/index.ts",
    "ts-example:start_js": "node dist/index_js.js",
    "ts-example:build": "npm run clean && babel ./src/ts-example --extensions \".ts,.tsx,.js,.jsx\" --out-dir dist",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "loki": "loki test --targetFilter=chrome.app",
    "loki:update": "loki update --targetFilter=chrome.app",
    "loki-docker": "loki test --targetFilter=chrome.docker --chromeDockerImage=yukinying/chrome-headless-browser:90.0.4408.0",
    "loki-docker:update": "loki update --targetFilter=chrome.docker --chromeDockerImage=yukinying/chrome-headless-browser:90.0.4408.0",
    "loki:approve": "loki approve"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/builder-webpack5": "^6.3.10",
    "@storybook/manager-webpack5": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.5",
    "jsdom": "^18.0.0",
    "jsdom-global": "^3.0.2",
    "loki": "^0.28.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "webpack": "^5.56.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1"
  }
}
